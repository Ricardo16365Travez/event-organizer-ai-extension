{
    "manifest_version": 3,
    "name": "Event Organizer AI",
    "version": "1.0",
    "description": "Extensión con IA para la gestión de eventos.",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": "icons/icon1.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content/content.js"]
      }
    ]
  }
  