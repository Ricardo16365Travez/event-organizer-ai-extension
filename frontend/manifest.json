{
  "name": "Event Organizer AI",
  "version": "1.0",
  "description": "Extensi√≥n para gestionar eventos",
  "manifest_version": 3,
  "permissions": ["storage"],
  "host_permissions": ["http://localhost:8000/*"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
